		<div id="main-container">
			<div id="command-bar">
				<audio-player></audio-player>
			</div>
			<div id="middle-container">
				<playlist-explorer id="playlist-explorer"></playlist-explorer>
				<control-bar></control-bar>
				<div id="right-section">
					<div id="breadcrumb-container">
						<bread-crumb id="breadcrumb"></bread-crumb>
					</div>
					<div id="file-explorer">
						<file-explorer id="explorer" is-visible="true" bind-to-favorites="true" explore-when-visible="false">
						</file-explorer>
					</div>
					<div id="favorites-container">
						<favorites-explorer></favorites-explorer>
					</div>
				</div>
			</div>
			<div id="queue-container">
				<media-queue></media-queue>
			</div>
		</div>

